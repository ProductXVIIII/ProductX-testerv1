if game.PlaceId == 3623096087 then
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Product X Tester", IntroText = "Product X by DeLTaXVIIII", SaveConfig = false, ConfigFolder = "DeLTaXVIIII Coinfig"})

    -- Variables
    getgenv().teleport = true
    getgenv().selectisland = "Tiny Island"
    getgenv().autohatch = true
    getgenv().selectcrystal = "Blue Crystal"
    getgenv().fastPunch = false
    getgenv().autoFarm = false
    getgenv().autoWeight = false

    -- Fonction de téléportation
    function teleport()
        local args = {
            [1] = "travelToArea",
            [2] = workspace:WaitForChild("areaCircles"):WaitForChild("areaCircle")
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("areaTravelRemote"):InvokeServer(unpack(args))
    end

    -- Fonction de hatching
    function autohatch()
        local args = {
            [1] = "openCrystal",
            [2] = getgenv().selectcrystal
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("openCrystalRemote"):InvokeServer(unpack(args))
    end

    -- Fonction Fast Punch améliorée
    function fastPunch()
        print("Fast Punch activé")
        spawn(function()
            while getgenv().fastPunch do
                print("Essai de frapper...")
                -- Vérifie si le Remote existe avant de l'appeler
                local swingFistRemote = game:GetService("ReplicatedStorage"):FindFirstChild("rEvents"):FindFirstChild("swingFistRemote")
                if swingFistRemote then
                    local args = {
                        [1] = "swingFist"
                    }
                    -- Appel du remote avec un essai pour éviter les erreurs
                    local success, errorMessage = pcall(function()
                        swingFistRemote:InvokeServer(unpack(args))
                    end)

                    if not success then
                        print("Erreur lors de l'appel de swingFistRemote: " .. errorMessage)
                    end
                else
                    print("Erreur : swingFistRemote introuvable.")
                end
                task.wait(0.5) -- Ajuste cette valeur pour la vitesse des coups
            end
        end)
    end

    -- Fonction Auto Weight
    function autoWeight()
        print("Auto Weight activé")
        spawn(function()
            while getgenv().autoWeight do
                -- Vérifie si le remote weightRemote existe
                local weightRemote = game:GetService("ReplicatedStorage"):FindFirstChild("rEvents"):FindFirstChild("weightRemote")
                if weightRemote then
                    local args = {
                        [1] = "liftWeight"
                    }
                    -- Appel du remote avec un essai pour éviter les erreurs
                    local success, errorMessage = pcall(function()
                        weightRemote:InvokeServer(unpack(args))
                    end)

                    if not success then
                        print("Erreur lors de l'appel de weightRemote: " .. errorMessage)
                    end
                else
                    print("Erreur : weightRemote introuvable.")
                end
                task.wait(0.5) -- Ajuste cette valeur pour la vitesse de levage
            end
        end)
    end

    -- Fonction Auto Farm (Auto Clicker)
    function autoFarm()
        print("Auto Farm activé")
        spawn(function()
            while getgenv().autoFarm do
                -- Vérifie si le remote clickRemote existe
                local clickRemote = game:GetService("ReplicatedStorage"):FindFirstChild("rEvents"):FindFirstChild("clickRemote")
                if clickRemote then
                    -- Essaye d'appeler le remote pour cliquer
                    local success, errorMessage = pcall(function()
                        clickRemote:FireServer()  -- Appel de la fonction de clic
                    end)

                    -- Gestion des erreurs
                    if not success then
                        print("Erreur lors de l'appel de clickRemote: " .. errorMessage)
                    end
                else
                    print("Erreur : clickRemote introuvable.")
                end
                
                task.wait(0.1) -- Attendre un court instant avant le prochain clic
            end
        end)
    end

    -- Fonction Night Sky (Ciel de nuit sans brouillard et sans assombrir le sol)
    function nightSky()
        local Lighting = game:GetService("Lighting")
        
        Lighting.ClockTime = 0 -- Minuit pour un ciel sombre
        Lighting.Brightness = 2 -- Garder une bonne luminosité au sol
        Lighting.OutdoorAmbient = Color3.new(0.3, 0.3, 0.3) -- Légère lumière d'ambiance pour ne pas assombrir le sol
        Lighting.Ambient = Color3.new(0.3, 0.3, 0.3) -- Légère lumière d'ambiance

        local sky = Instance.new("Sky")
        sky.Parent = Lighting
        sky.SkyboxBk = "rbxassetid://6825465291" -- Ciel de nuit noir
        sky.SkyboxDn = "rbxassetid://6825465291"
        sky.SkyboxFt = "rbxassetid://6825465291"
        sky.SkyboxLf = "rbxassetid://6825465291"
        sky.SkyboxRt = "rbxassetid://6825465291"
        sky.SkyboxUp = "rbxassetid://6825465291"
    end

    -- Création des onglets
    local TeleportTab = Window:MakeTab({
        Name = "Teleport",
        Icon = "rbxassetid://4483345998"
    })

    local EggsTab = Window:MakeTab({
        Name = "Eggs",
        Icon = "rbxassetid://4483345998"
    })

    local MiscTab = Window:MakeTab({
        Name = "Misc",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    local AutoFarmTab = Window:MakeTab({
        Name = "Auto Farm",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- Boutons et Toggle
    TeleportTab:AddButton({
        Name = "Teleport",
        Callback = function()
            teleport()
        end
    })

    EggsTab:AddButton({
        Name = "Auto Hatch",
        Callback = function()
            autohatch()
        end
    })

    MiscTab:AddButton({
        Name = "Night Sky",
        Callback = function()
            nightSky()
        end
    })

    -- Toggle pour Fast Punch
    AutoFarmTab:AddToggle({
        Name = "Fast Punch",
        Default = false,
        Callback = function(value)
            getgenv().fastPunch = value
            if value then
                fastPunch()
            end
        end
    })

    -- Toggle pour Auto Weight
    AutoFarmTab:AddToggle({
        Name = "Auto Weight",
        Default = false,
        Callback = function(value)
            getgenv().autoWeight = value
            if value then
                autoWeight()  -- Appelle la fonction autoWeight si activé
            end
        end
    })

    -- Toggle pour Auto Farm (Auto Clicker)
    AutoFarmTab:AddToggle({
        Name = "Auto Farm",
        Default = false,
        Callback = function(value)
            getgenv().autoFarm = value
            if value then
                autoFarm()  -- Appelle la fonction autoFarm si activé
            end
        end
    })

    -- Dropdowns pour sélectionner l'île et les oeufs
    TeleportTab:AddDropdown({
        Name = "Select Island",
        Default = "Tiny Island",
        Options = {"Main Island", "Tiny Island", "Frost Island", "Mythical Island","Eternal Island","Legends Island","Muscleking Island"},
        Callback = function(Value)
            getgenv().selectisland = Value
            print(getgenv().selectisland)
        end
    })

    EggsTab:AddDropdown({
        Name = "Select Egg",
        Default = "Blue Crystal",
        Options = {"Blue Crystal"},
        Callback = function(Value)
            getgenv().selectcrystal = Value
            print(getgenv().selectcrystal)
        end
    })

    -- Anti-AFK
    MiscTab:AddButton({
        Name = "Anti AFK",
        Callback = function()
            local VirtualInputManager = game:GetService("VirtualInputManager")
            local UserInputService = game:GetService("UserInputService")

            -- Code pour anti-AFK
            while true do
                VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Unknown, false, game)
                task.wait(math.random(15, 120)) -- Temps aléatoire avant le prochain envoi d'événement
            end
        end
    })

    -- Initialiser la librairie Orion
    OrionLib:Init()
end
