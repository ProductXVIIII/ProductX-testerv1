if game.PlaceId == 3623096087 then
    local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
    local Window = OrionLib:MakeWindow({
        Name = "Product X ðŸ’ªMuscle Legends",
        IntroText = "Product X by DeLTaXVIIII",
        SaveConfig = false,
        ConfigFolder = "DeLTaXVIIII_Config"
    })

    -- Global Variables
    getgenv().selectisland = "Tiny Island"
    getgenv().selectcrystal = "Blue Crystal"

    -- Functions
    function teleport()
        local island = getgenv().selectisland
        local args = {
            [1] = "travelToArea",
            [2] = workspace:WaitForChild("areaCircles"):FindFirstChild(island)
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("areaTravelRemote"):InvokeServer(unpack(args))
    end

    function autohatch()
        local args = {
            [1] = "openCrystal",
            [2] = getgenv().selectcrystal
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("openCrystalRemote"):InvokeServer(unpack(args))
    end

    -- Tabs and UI Elements
    local TeleportTab = Window:MakeTab({
        Name = "Teleport",
        Icon = "rbxassetid://4483345998"
    })

    local EggsTab = Window:MakeTab({
        Name = "Eggs",
        Icon = "rbxassetid://4483345998"
    })

    -- Teleport Button
    TeleportTab:AddButton({
        Name = "Teleport",
        Callback = function()
            teleport()
        end
    })

    -- Auto Hatch Button
    EggsTab:AddButton({
        Name = "Auto Hatch",
        Callback = function()
            autohatch()
        end
    })

    -- Island Selection Dropdown
    TeleportTab:AddDropdown({
        Name = "Select Island",
        Default = "Tiny Island",
        Options = {"Main Island", "Tiny Island", "Frost Island", "Mythical Island", "Eternal Island", "Legends Island", "Muscleking Island"},
        Callback = function(Value)
            getgenv().selectisland = Value
            print("Selected Island:", Value)
        end
    })

    -- Egg Selection Dropdown
    EggsTab:AddDropdown({
        Name = "Select Egg",
        Default = "Blue Crystal",
        Options = {"Blue Crystal"},
        Callback = function(Value)
            getgenv().selectcrystal = Value
            print("Selected Crystal:", Value)
        end
    })

    -- Miscellaneous Tab
    local MiscTab = Window:MakeTab({
        Name = "Misc",
        Icon = "rbxassetid://4483345998"
    })

    -- Anti-AFK Button
    MiscTab:AddButton({
        Name = "Anti AFK",
        Callback = function()
            local VirtualUser = game:GetService("VirtualUser")
            game:GetService("Players").LocalPlayer.Idled:connect(function()
                VirtualUser:CaptureController()
                VirtualUser:ClickButton2(Vector2.new())
            end)
        end
    })

    -- Night Sky Button
    MiscTab:AddButton({
        Name = "Night Sky",
        Callback = function()
            local Lighting = game:GetService("Lighting")

            -- Set Nighttime Settings
            Lighting.ClockTime = 0
            Lighting.Brightness = 1
            Lighting.OutdoorAmbient = Color3.new(0.1, 0.1, 0.2)
            Lighting.Ambient = Color3.new(0.1, 0.1, 0.2)

            -- Add Color Correction Effect for Night Look
            local colorCorrection = Instance.new("ColorCorrectionEffect")
            colorCorrection.Parent = Lighting
            colorCorrection.TintColor = Color3.new(0.3, 0.3, 0.4)
            colorCorrection.Saturation = -0.2
            colorCorrection.Brightness = -0.1

            -- Adjust Fog for Nighttime Atmosphere
            Lighting.FogColor = Color3.new(0, 0, 0.1)
            Lighting.FogEnd = 1000
            Lighting.FogStart = 200
        end
    })

    -- Initialize Orion Library
    OrionLib:Init()
end
