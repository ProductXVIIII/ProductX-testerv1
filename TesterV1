AutofarmTab = Window:MakeTab({
	Name = "Auto farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

--[[
Name = <string>
Icon = <string>
PremiumOnly = <bool>
]]

AutofarmTab:AddButton({
	Name = "Fast Punch",
	Callback = function()

  local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local punching = false
local punchSpeed = 0.1  -- temps en secondes entre les coups de poing

function fastPunch()
    if punching then return end  -- pour √©viter de r√©p√©ter si d√©j√† en cours
    punching = true
    while punching do
        -- Ex√©cute l'animation du coup de poing ici
        local punchAnimation = Instance.new("Animation")
        punchAnimation.AnimationId = "rbxassetid://ID_DE_VOTRE_ANIMATION"
        local animationTrack = character.Humanoid:LoadAnimation(punchAnimation)
        animationTrack:Play()
        
        -- Ajouter ici des effets visuels ou sons si souhait√©
        
        wait(punchSpeed)  -- attente avant le prochain coup
    end
end

function stopPunch()
    punching = false
end

-- Associe les touches pour commencer et arr√™ter le "fast punch"
player.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.P then  -- touche P pour commencer
        fastPunch()
    end
end)

player.InputEnded:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.P then  -- rel√¢chement de P pour arr√™ter
        stopPunch()
    end
end)


end
OrionLib:Init()
