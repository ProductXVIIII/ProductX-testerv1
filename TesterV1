if game.PlaceId == 3623096087 then
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Product X Tester", IntroText = "Product X by DeLTaXVIIII", SaveConfig = false, ConfigFolder = "DeLTaXVIIII Coinfig"})

    -- Variables
    getgenv().teleport = true
    getgenv().selectisland = "Tiny Island"
    getgenv().autohatch = true
    getgenv().selectcrystal = "Blue Crystal"
    getgenv().fastPunch = false
    getgenv().autoPunch = false
    getgenv().fastWeight = false

    -- Fonction de téléportation automatique
    function teleport()
        local args = {
            [1] = "travelToArea",
            [2] = workspace:WaitForChild("areaCircles"):WaitForChild("areaCircle")
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("areaTravelRemote"):InvokeServer(unpack(args))
    end

    -- Fonction de hatching
    function autohatch()
        local args = {
            [1] = "openCrystal",
            [2] = getgenv().selectcrystal
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("openCrystalRemote"):InvokeServer(unpack(args))
    end

    -- Fonction Fast Punch améliorée
    function fastPunch()
        print("Fast Punch activé")
        spawn(function()
            while getgenv().fastPunch do
                local punchValue = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Punch"):FindFirstChildOfClass("NumberValue")
                if punchValue then
                    punchValue.Value = 0 -- Réinitialise la valeur à 0 pour accélérer les coups
                    print("Fast Punch en cours...")
                else
                    print("Erreur : Punch introuvable dans le Backpack.")
                end
                task.wait(0.5) -- Fréquence des coups
            end
        end)
    end

    -- Nouveau code pour l'Auto Punch en utilisant l'auto-clicker fourni
    function autoPunch()
        local clicks = {}
        
        -- Configuration des clics
        for i = 1, 5 do -- Changez 5 pour le nombre de clics désiré
            clicks[i] = {location = Vector3.new(0, 0, 0), wait = 1, continuous = false, randomize = false} -- exemple
        end

        local offset = 0 -- Changez l'offset si nécessaire

        spawn(function()
            while getgenv().autoPunch do
                for i = 1, #clicks do
                    local loc = clicks[i].location
                    if clicks[i].randomize then
                        local randomOffsetX = math.random(-offset, offset)
                        local randomOffsetY = math.random(-offset, offset)
                        loc = loc + Vector3.new(randomOffsetX, randomOffsetY, 0)
                    end
                    print("Clic "..i.."  de "..#clicks)
                    
                    if clicks[i].continuous then
                        print("Clic continu à : ", loc)
                        -- Ajouter la logique continue si nécessaire
                    else
                        print("Clic unique à : ", loc)
                        -- Exécuter le clic unique ici
                    end
                    wait(clicks[i].wait)
                end
            end
        end)
    end

    -- Fonction Fast Weight
    function fastWeight()
        print("Fast Weight activé")
        spawn(function()
            while getgenv().fastWeight do
                local weightValue = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Weight"):FindFirstChildOfClass("NumberValue")
                if weightValue then
                    weightValue.Value = 0 -- Réinitialise la valeur à 0 pour accélérer l’utilisation du poids
                    print("Fast Weight en cours...")
                else
                    print("Erreur : Weight introuvable dans le Backpack.")
                end
                task.wait(0.5) -- Fréquence de levage
            end
        end)
    end

    -- Fonction Night Sky
    function nightSky()
        local Lighting = game:GetService("Lighting")
        
        Lighting.ClockTime = 0
        Lighting.Brightness = 2
        Lighting.OutdoorAmbient = Color3.new(0.3, 0.3, 0.3)
        Lighting.Ambient = Color3.new(0.3, 0.3, 0.3)

        local sky = Instance.new("Sky")
        sky.Parent = Lighting
        sky.SkyboxBk = "rbxassetid://6825465291"
        sky.SkyboxDn = "rbxassetid://6825465291"
        sky.SkyboxFt = "rbxassetid://6825465291"
        sky.SkyboxLf = "rbxassetid://6825465291"
        sky.SkyboxRt = "rbxassetid://6825465291"
        sky.SkyboxUp = "rbxassetid://6825465291"
    end

    -- Création des onglets et UI
    local TeleportTab = Window:MakeTab({ Name = "Teleport", Icon = "rbxassetid://4483345998" })
    local EggsTab = Window:MakeTab({ Name = "Eggs", Icon = "rbxassetid://4483345998" })
    local MiscTab = Window:MakeTab({ Name = "Misc", Icon = "rbxassetid://4483345998" })
    local AutoFarmTab = Window:MakeTab({ Name = "Auto Farm", Icon = "rbxassetid://4483345998" })

    TeleportTab:AddButton({ Name = "Teleport", Callback = teleport })
    EggsTab:AddButton({ Name = "Auto Hatch", Callback = autohatch })
    MiscTab:AddButton({ Name = "Night Sky", Callback = nightSky })

    -- Toggle pour l'Auto Punch
    AutoFarmTab:AddToggle({
        Name = "Auto Punch",
        Default = false,
        Callback = function(value)
            getgenv().autoPunch = value
            if value then autoPunch() end
        end
    })

    -- Initialiser la librairie Orion
    OrionLib:Init()
end
