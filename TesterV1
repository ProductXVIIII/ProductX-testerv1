if game.PlaceId == 3623096087 then
    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Product X Tester", IntroText = "Product X by DeLTaXVIIII", SaveConfig = false, ConfigFolder = "DeLTaXVIIII Coinfig"})

    -- Variables
    getgenv().teleport = true
    getgenv().selectisland = "Tiny Island"
    getgenv().autohatch = true
    getgenv().selectcrystal = "Blue Crystal"
    getgenv().fastPunch = false
    getgenv().autoFarm = false
    getgenv().autoWeight = false

    -- Fonction de téléportation
    function teleport()
        local args = {
            [1] = "travelToArea",
            [2] = workspace:WaitForChild("areaCircles"):WaitForChild("areaCircle")
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("areaTravelRemote"):InvokeServer(unpack(args))
    end

    -- Fonction de hatching
    function autohatch()
        local args = {
            [1] = "openCrystal",
            [2] = getgenv().selectcrystal
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("openCrystalRemote"):InvokeServer(unpack(args))
    end

    -- Fonction Fast Punch améliorée
    function fastPunch()
        print("Fast Punch activé")

        -- Vérifie et définit NumberValue à 0 pour Punch
        local player = game:GetService("Players").LocalPlayer
        local backpack = player:FindFirstChild("Backpack")
        local punchTool = backpack and backpack:FindFirstChild("Punch")

        if punchTool then
            local numberValue = punchTool:FindFirstChildOfClass("NumberValue")
            if numberValue then
                numberValue.Value = 0
                print("La valeur de Punch a été définie à 0.")
            else
                warn("NumberValue introuvable dans Punch.")
            end
        else
            warn("Outil Punch introuvable dans le Backpack.")
        end

        -- Démarre la boucle de frappe rapide
        spawn(function()
            while getgenv().fastPunch do
                print("Essai de frapper...")
                local swingFistRemote = game:GetService("ReplicatedStorage"):FindFirstChild("rEvents"):FindFirstChild("swingFistRemote")
                if swingFistRemote then
                    local args = {
                        [1] = "swingFist"
                    }
                    local success, errorMessage = pcall(function()
                        swingFistRemote:InvokeServer(unpack(args))
                    end)

                    if not success then
                        print("Erreur lors de l'appel de swingFistRemote: " .. errorMessage)
                    end
                else
                    print("Erreur : swingFistRemote introuvable.")
                end
                task.wait(0.5)
            end
        end)
    end

    -- Fonction Auto Weight
    function autoWeight()
        print("Auto Weight activé")

        -- Vérifie et définit NumberValue à 0 pour Weight
        local player = game:GetService("Players").LocalPlayer
        local backpack = player:FindFirstChild("Backpack")
        local weightTool = backpack and backpack:FindFirstChild("Weight")

        if weightTool then
            local numberValue = weightTool:FindFirstChildOfClass("NumberValue")
            if numberValue then
                numberValue.Value = 0
                print("La valeur de Weight a été définie à 0.")
            else
                warn("NumberValue introuvable dans Weight.")
            end
        else
            warn("Outil Weight introuvable dans le Backpack.")
        end

        -- Démarre la boucle de levage de poids
        spawn(function()
            while getgenv().autoWeight do
                print("Essai de lever le poids...")
                local liftWeightRemote = game:GetService("ReplicatedStorage"):FindFirstChild("rEvents"):FindFirstChild("liftWeightRemote")
                if liftWeightRemote then
                    local args = {
                        [1] = "liftWeight"
                    }
                    local success, errorMessage = pcall(function()
                        liftWeightRemote:InvokeServer(unpack(args))
                    end)

                    if not success then
                        print("Erreur lors de l'appel de liftWeightRemote: " .. errorMessage)
                    end
                else
                    print("Erreur : liftWeightRemote introuvable.")
                end
                task.wait(0.5)
            end
        end)
    end

    -- Fonction Night Sky
    function nightSky()
        local Lighting = game:GetService("Lighting")

        Lighting.ClockTime = 0
        Lighting.Brightness = 2
        Lighting.OutdoorAmbient = Color3.new(0.3, 0.3, 0.3)
        Lighting.Ambient = Color3.new(0.3, 0.3, 0.3)

        local sky = Instance.new("Sky")
        sky.Parent = Lighting
        sky.SkyboxBk = "rbxassetid://6825465291"
        sky.SkyboxDn = "rbxassetid://6825465291"
        sky.SkyboxFt = "rbxassetid://6825465291"
        sky.SkyboxLf = "rbxassetid://6825465291"
        sky.SkyboxRt = "rbxassetid://6825465291"
        sky.SkyboxUp = "rbxassetid://6825465291"
    end

    -- Fonction Auto Farm (Auto Clicker)
    function autoFarm()
        print("Auto Farm activé")
        spawn(function()
            while getgenv().autoFarm do
                local clickRemote = game:GetService("ReplicatedStorage"):FindFirstChild("rEvents"):FindFirstChild("clickRemote")
                if clickRemote then
                    local success, errorMessage = pcall(function()
                        clickRemote:FireServer()
                    end)

                    if not success then
                        print("Erreur lors de l'appel de clickRemote: " .. errorMessage)
                    end
                else
                    print("Erreur : clickRemote introuvable.")
                end
                task.wait(0.1)
            end
        end)
    end

    -- Création des onglets
    local TeleportTab = Window:MakeTab({
        Name = "Teleport",
        Icon = "rbxassetid://4483345998"
    })

    local EggsTab = Window:MakeTab({
        Name = "Eggs",
        Icon = "rbxassetid://4483345998"
    })

    local MiscTab = Window:MakeTab({
        Name = "Misc",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    local AutoFarmTab = Window:MakeTab({
        Name = "Auto Farm",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- Boutons et Toggle
    TeleportTab:AddButton({
        Name = "Teleport",
        Callback = function()
            teleport()
        end
    })

    EggsTab:AddButton({
        Name = "Auto Hatch",
        Callback = function()
            autohatch()
        end
    })

    MiscTab:AddButton({
        Name = "Night Sky",
        Callback = function()
            nightSky()
        end
    })

    -- Toggle pour Fast Punch
    AutoFarmTab:AddToggle({
        Name = "Fast Punch",
        Default = false,
        Callback = function(value)
            getgenv().fastPunch = value
            if value then
                fastPunch()
            end
        end
    })

    -- Toggle pour Auto Farm (Auto Clicker)
    AutoFarmTab:AddToggle({
        Name = "Auto Farm",
        Default = false,
        Callback = function(value)
            getgenv().autoFarm = value
            if value then
                autoFarm()
            end
        end
    })

    -- Toggle pour Auto Weight
    AutoFarmTab:AddToggle({
        Name = "Auto Weight",
        Default = false,
        Callback = function(value)
            getgenv().autoWeight = value
            if value then
                autoWeight()
            end
        end
    })

    -- Dropdowns pour sélectionner l'île et les oeufs
    TeleportTab:AddDropdown({
        Name = "Select Island",
        Default = "Tiny Island",
        Options = {"Main Island", "Tiny Island", "Frost Island", "Mythical Island", "Eternal Island", "Legends Island", "Muscleking Island"},
        Callback = function(Value)
            getgenv().selectisland = Value
            print(getgenv().selectisland)
        end
    })

    EggsTab:AddDropdown({
        Name = "Select Egg",
        Default = "Blue Crystal",
        Options = {"Blue Crystal"},
        Callback = function(Value)
            getgenv().selectcrystal = Value
            print(getgenv().selectcrystal)
        end
    })

    -- Anti-AFK
    MiscTab:AddButton({
        Name = "Anti AFK",
        Callback = function()
            local VirtualInputManager = game:GetService("VirtualInputManager")
            local UserInputService = game:GetService("UserInputService")

            -- Code pour anti-AFK
            -- Code pour anti-AFK
            for i, v in next, game.Players.LocalPlayer.PlayerGui:GetChildren() do
                if v:IsA("ScreenGui") then
                    VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.W, false, game)
                    VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.W, false, game)
                    task.wait(1)
                end
            end
        end
    })

    -- Afficher la fenêtre
    OrionLib:Init()
end
