if game.PlaceId == 3623096087 then
    local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Orion/main/source'))()
    local Window = OrionLib:MakeWindow({
        Name = "Product X üí™Muscle Legends",
        IntroText = "Product X by DeLTaXVIIII",
        SaveConfig = false,
        ConfigFolder = "DeLTaXVIIII_Config"
    })

    -- Variables Globales
    getgenv().selectisland = "Tiny Island"
    getgenv().selectcrystal = "Blue Crystal"

    -- Fonction de t√©l√©portation
    function teleport()
        local selectedIsland = getgenv().selectisland
        local areaCircle = workspace:WaitForChild("areaCircles"):FindFirstChild(selectedIsland)

        if areaCircle then
            local args = {
                [1] = "travelToArea",
                [2] = areaCircle
            }
            game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("areaTravelRemote"):InvokeServer(unpack(args))
        else
            warn("L'√Æle s√©lectionn√©e n'existe pas : " .. tostring(selectedIsland))
        end
    end

    -- Fonction pour auto-hatch
    function autohatch()
        local crystalName = getgenv().selectcrystal
        local args = {
            [1] = "openCrystal",
            [2] = crystalName
        }
        game:GetService("ReplicatedStorage"):WaitForChild("rEvents"):WaitForChild("openCrystalRemote"):InvokeServer(unpack(args))
    end

    -- Fonction Night Sky (Ciel de nuit sans brouillard)
    function nightSky()
        local Lighting = game:GetService("Lighting")

        Lighting.ClockTime = 0 -- Minuit
        Lighting.Brightness = 1
        Lighting.OutdoorAmbient = Color3.new(0.1, 0.1, 0.2)
        Lighting.Ambient = Color3.new(0.1, 0.1, 0.2)

        local colorCorrection = Instance.new("ColorCorrectionEffect")
        colorCorrection.Parent = Lighting
        colorCorrection.TintColor = Color3.new(0.3, 0.3, 0.4)
        colorCorrection.Saturation = -0.2
        colorCorrection.Brightness = -0.1
    end

    -- Tabs et √©l√©ments de l'UI
    local TeleportTab = Window:MakeTab({
        Name = "Teleport",
        Icon = "rbxassetid://4483345998"
    })

    local EggsTab = Window:MakeTab({
        Name = "Eggs",
        Icon = "rbxassetid://4483345998"
    })

    local MiscTab = Window:MakeTab({
        Name = "Misc",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    -- Bouton de t√©l√©portation
    TeleportTab:AddButton({
        Name = "T√©l√©portation",
        Callback = function()
            teleport()
        end
    })

    -- Bouton pour auto-hatch
    EggsTab:AddButton({
        Name = "Auto Hatch",
        Callback = function()
            autohatch()
        end
    })

    -- Bouton pour activer le ciel de nuit
    MiscTab:AddButton({
        Name = "Activer le ciel de nuit",
        Callback = function()
            nightSky()
        end
    })

    -- Dropdown de s√©lection d'√Æle
    TeleportTab:AddDropdown({
        Name = "S√©lectionner l'√Æle",
        Default = "Tiny Island",
        Options = {"Main Island", "Tiny Island", "Frost Island", "Mythical Island", "Eternal Island", "Legends Island", "Muscleking Island"},
        Callback = function(Value)
            getgenv().selectisland = Value
            print("√éle s√©lectionn√©e : ", Value)
        end
    })

    -- Dropdown de s√©lection d'≈ìuf
    EggsTab:AddDropdown({
        Name = "S√©lectionner l'≈ìuf",
        Default = "Blue Crystal",
        Options = {"Blue Crystal"},
        Callback = function(Value)
            getgenv().selectcrystal = Value
            print("Cristal s√©lectionn√© : ", Value)
        end
    })

    -- Initialisation de la librairie Orion
    OrionLib:Init()
end
